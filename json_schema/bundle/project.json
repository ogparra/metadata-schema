{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": false,
    "description": "A schema for a project bundle.",
    "id": "https://schema.humancellatlas.org/bundle/5.1.0/project",
    "properties": {
        "content": {
            "$ref": "https://schema.humancellatlas.org/type/project/5.1.0/project",
            "description": "Content for a project type entity.",
            "type": "object"
        },
        "describedBy": {
            "description": "The URL reference to the schema.",
            "pattern": "https://schema.humancellatlas.org/bundle/[0-9]{1,}.[0-9]{1,}.[0-9]{1,}/project",
            "type": "string"
        },
        "hca_ingest": {
            "$ref": "https://schema.humancellatlas.org/bundle/5.1.0/ingest_audit",
            "description": "Core fields added by HCA ingest service",
            "type": "object"
        },
        "schema_type": {
            "description": "The type of the metadata schema entity.",
            "enum": [
                "project_bundle"
            ],
            "type": "string"
        },
        "schema_version": {
            "description": "The version number of the schema in major.minor.patch format.",
            "pattern": "^[0-9]{1,}.[0-9]{1,}.[0-9]{1,}$",
            "type": "string"
        }
    },
    "required": [
        "describedBy",
        "schema_type",
        "hca_ingest",
        "content"
    ],
    "title": "project",
    "type": "object"
}