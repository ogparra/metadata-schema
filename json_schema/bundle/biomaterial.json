{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": false,
    "definitions": {
        "biomaterial_ingest": {
            "properties": {
                "content": {
                    "description": "Content for any biomaterial type entity.",
                    "oneOf": [
                        {
                            "$ref": "https://schema.humancellatlas.org/type/biomaterial/5.1.2/donor_organism"
                        },
                        {
                            "$ref": "https://schema.humancellatlas.org/type/biomaterial/5.1.2/specimen_from_organism"
                        },
                        {
                            "$ref": "https://schema.humancellatlas.org/type/biomaterial/6.0.1/cell_suspension"
                        },
                        {
                            "$ref": "https://schema.humancellatlas.org/type/biomaterial/5.1.2/organoid"
                        },
                        {
                            "$ref": "https://schema.humancellatlas.org/type/biomaterial/5.1.2/cell_line"
                        }
                    ],
                    "type": "object"
                },
                "hca_ingest": {
                    "$ref": "https://schema.humancellatlas.org/bundle/5.1.0/ingest_audit",
                    "description": "Core fields added by HCA ingest service",
                    "type": "object"
                }
            },
            "required": [
                "hca_ingest",
                "content"
            ],
            "type": "object"
        }
    },
    "description": "A schema for a biomaterial bundle.",
    "id": "https://schema.humancellatlas.org/bundle/5.1.2/biomaterial",
    "properties": {
        "biomaterials": {
            "description": "An array of biomaterials.",
            "items": {
                "$ref": "https://schema.humancellatlas.org/bundle/5.1.2/biomaterial#/definitions/biomaterial_ingest"
            },
            "type": "array"
        },
        "describedBy": {
            "description": "The URL reference to the schema.",
            "pattern": "https://schema.humancellatlas.org/bundle/[0-9]{1,}.[0-9]{1,}.[0-9]{1,}/biomaterial",
            "type": "string"
        },
        "schema_type": {
            "description": "The type of the metadata schema entity.",
            "enum": [
                "biomaterial_bundle"
            ],
            "type": "string"
        },
        "schema_version": {
            "description": "The version number of the schema in major.minor.patch format.",
            "pattern": "^[0-9]{1,}.[0-9]{1,}.[0-9]{1,}$",
            "type": "string"
        }
    },
    "required": [
        "describedBy",
        "schema_type"
    ],
    "title": "biomaterial",
    "type": "object"
}