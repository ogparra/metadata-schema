{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": false,
    "description": "A file analysis results produced by a secondary analysis pipeline.",
    "id": "https://schema.humancellatlas.org/type/file/5.1.0/analysis_file",
    "properties": {
        "describedBy": {
            "description": "The URL reference to the schema.",
            "pattern": "https://schema.humancellatlas.org/type/file/[0-9]{1,}.[0-9]{1,}.[0-9]{1,}/analysis_file",
            "type": "string"
        },
        "file_core": {
            "$ref": "https://schema.humancellatlas.org/core/file/5.1.0/file_core",
            "description": "Core file-level information.",
            "type": "object"
        },
        "schema_type": {
            "description": "The type of the metadata schema entity.",
            "enum": [
                "file"
            ],
            "type": "string"
        },
        "schema_version": {
            "description": "The version number of the schema in major.minor.patch format.",
            "example": "4.6.1",
            "pattern": "^[0-9]{1,}.[0-9]{1,}.[0-9]{1,}$",
            "type": "string"
        }
    },
    "required": [
        "describedBy",
        "schema_type",
        "file_core"
    ],
    "title": "analysis_file",
    "type": "object"
}