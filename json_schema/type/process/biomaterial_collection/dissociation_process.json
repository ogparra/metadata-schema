{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": false,
    "description": "Contains information on the dissociation process used to separate the cells in a specimen.",
    "id": "https://schema.humancellatlas.org/type/process/biomaterial_collection/5.1.0/dissociation_process",
    "properties": {
        "describedBy": {
            "description": "The URL reference to the schema.",
            "pattern": "https://schema.humancellatlas.org/type/process/biomaterial_collection/[0-9]{1,}.[0-9]{1,}.[0-9]{1,}/dissociation_process",
            "type": "string"
        },
        "dissociation_method": {
            "description": "How cells or organelles were dissociated.",
            "enum": [
                "10x_v2",
                "FACS",
                "Fluidigm C1",
                "drop-seq",
                "inDrop",
                "mouth pipette",
                "bulk",
                "enzymatic",
                "mechanical",
                "none"
            ],
            "type": "string",
            "user_friendly": "Dissociation method"
        },
        "nucleic_acid_source": {
            "description": "Source cells or organelles from which nucleic acid molecules were collected.",
            "enum": [
                "bulk cell",
                "single cell",
                "single nucleus",
                "bulk nuclei",
                "mitochondria"
            ],
            "type": "string",
            "user_friendly": "Nucleic acid source"
        },
        "process_core": {
            "$ref": "https://schema.humancellatlas.org/core/process/5.1.0/process_core",
            "description": "Core process-level information.",
            "type": "object"
        },
        "process_reagents": {
            "description": "A list of purchased reagents used in this process.",
            "items": {
                "$ref": "https://schema.humancellatlas.org/module/process/5.2.0/purchased_reagents"
            },
            "type": "array"
        },
        "process_type": {
            "$ref": "https://schema.humancellatlas.org/module/ontology/5.1.0/process_type_ontology",
            "description": "The type of process. Should be a child term of https://www.ebi.ac.uk/ols/ontologies/efo/terms?iri=http%3A%2F%2Fwww.ebi.ac.uk%2Fefo%2FEFO_0002694.",
            "type": "object",
            "user_friendly": "Process type"
        },
        "schema_type": {
            "description": "The type of the metadata schema entity.",
            "enum": [
                "process"
            ],
            "type": "string"
        },
        "schema_version": {
            "description": "The version number of the schema in major.minor.patch format.",
            "example": "4.6.1",
            "pattern": "^[0-9]{1,}.[0-9]{1,}.[0-9]{1,}$",
            "type": "string"
        }
    },
    "required": [
        "describedBy",
        "schema_type",
        "process_core",
        "dissociation_method"
    ],
    "title": "dissociation_process",
    "type": "object"
}