{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": true,
    "definitions": {
        "parameter": {
            "additionalProperties": false,
            "properties": {
                "checksum": {
                    "type": "string"
                },
                "parameter_name": {
                    "type": "string"
                },
                "parameter_value": {
                    "type": "string"
                }
            },
            "required": [
                "parameter_name",
                "parameter_value"
            ],
            "type": "object"
        },
        "task": {
            "additionalProperties": false,
            "properties": {
                "cpus": {
                    "type": "integer"
                },
                "disk_size": {
                    "type": "string"
                },
                "docker_image": {
                    "type": "string"
                },
                "log_err": {
                    "type": "string"
                },
                "log_out": {
                    "type": "string"
                },
                "memory": {
                    "type": "string"
                },
                "start_time": {
                    "format": "date-time",
                    "type": "string"
                },
                "stop_time": {
                    "format": "date-time",
                    "type": "string"
                },
                "task_name": {
                    "type": "string"
                },
                "zone": {
                    "type": "string"
                }
            },
            "required": [
                "task_name",
                "start_time",
                "stop_time",
                "disk_size",
                "docker_image",
                "cpus",
                "memory",
                "zone"
            ],
            "type": "object"
        }
    },
    "description": "Information about an analysis that was performed on data files.",
    "id": "https://schema.humancellatlas.org/type/process/analysis/5.1.0/analysis_process",
    "properties": {
        "analysis_run_type": {
            "description": "Indicator of whether the analysis actually ran or was just copied forward as an optimization.",
            "enum": [
                "run",
                "copy-forward"
            ],
            "type": "string"
        },
        "computational_method": {
            "description": "A URI to a versioned workflow and versioned execution environment in a GA4GH-compliant repository.",
            "type": "string"
        },
        "describedBy": {
            "description": "The URL reference to the schema.",
            "pattern": "https://schema.humancellatlas.org/type/process/analysis/[0-9]{1,}.[0-9]{1,}.[0-9]{1,}/analysis_process",
            "type": "string"
        },
        "input_bundles": {
            "description": "The input bundles used in this analysis run.",
            "items": {
                "type": "string"
            },
            "type": "array"
        },
        "inputs": {
            "description": "Input parameters used in the pipeline run, these can be files or string values (settings).",
            "items": {
                "$ref": "https://schema.humancellatlas.org/type/process/analysis/5.1.0/analysis_process#/definitions/parameter"
            },
            "type": "array"
        },
        "outputs": {
            "description": "Output generated by the pipeline run.",
            "items": {
                "$ref": "https://schema.humancellatlas.org/type/file/5.1.0/analysis_file",
                "type": "object"
            },
            "type": "array"
        },
        "process_core": {
            "$ref": "https://schema.humancellatlas.org/core/process/5.1.0/process_core",
            "description": "Core process-level information.",
            "type": "object"
        },
        "process_type": {
            "$ref": "https://schema.humancellatlas.org/module/ontology/5.1.0/process_type_ontology",
            "description": "The type of process. Should be a child term of https://www.ebi.ac.uk/ols/ontologies/efo/terms?iri=http%3A%2F%2Fwww.ebi.ac.uk%2Fefo%2FEFO_0002694.",
            "type": "object"
        },
        "reference_bundle": {
            "description": "Bundle containing the reference used in running the pipeline.",
            "type": "string"
        },
        "schema_type": {
            "description": "The type of the metadata schema entity.",
            "enum": [
                "process"
            ],
            "type": "string"
        },
        "schema_version": {
            "description": "The version number of the schema in major.minor.patch format.",
            "example": "4.6.1",
            "pattern": "^[0-9]{1,}.[0-9]{1,}.[0-9]{1,}$",
            "type": "string"
        },
        "tasks": {
            "description": "Descriptions of tasks in the workflow.",
            "items": {
                "$ref": "https://schema.humancellatlas.org/type/process/analysis/5.1.0/analysis_process#/definitions/task"
            },
            "type": "array"
        },
        "timestamp_start_utc": {
            "description": "Initial start time of the full pipeline.",
            "format": "date-time",
            "type": "string"
        },
        "timestamp_stop_utc": {
            "description": "Terminal stop time of the full pipeline.",
            "format": "date-time",
            "type": "string"
        }
    },
    "required": [
        "describedBy",
        "schema_type",
        "timestamp_start_utc",
        "timestamp_stop_utc",
        "computational_method",
        "input_bundles",
        "reference_bundle",
        "analysis_run_type",
        "tasks",
        "inputs",
        "outputs"
    ],
    "title": "analysis_process",
    "type": "object"
}